<soundparameters>
	<volumes>
		<!--
		 Describes the volumes of files in %
		 This is more flexible/easier than modifying the files
		 Modifying volume of individual files is not supported (yet)
		 This should not be a problem, as sounds for the same "clap" should have the
		 same source...
		-->
		<volume dir="percusion/foot/" volume="25"/>
		<volume dir="palmas/sonora/fuerte/" volume="25"/>
		<volume dir="palmas/sonora/baja/" volume="25"/>
	</volumes>
</soundparameters>
